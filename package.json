  "homepage": "https://puissance-4-react-node-app.herokuapp.com/",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jwt-simple": "^0.5.6",
    "mongo-sanitize": "^1.0.1",
    "mongoose": "^5.13.15",
    "nodemon": "^2.0.2"
